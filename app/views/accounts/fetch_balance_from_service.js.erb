<% if @response %>
  $('#account-balance').html('<%= number_to_currency(@account.balance, precision: 2) %>');
  $('#balance-as-of-date').attr('data-datetime', '<%= @account.balance_as_of_date.to_i %>');
  applicationHandler.humanizeDateTimeFields();
<% else %>
  $('#account-balance').html('Sorry, please try again.');
<% end %>